# Cloudflare R2 Configuration
CLOUDFLARE_R2_ENDPOINT=https://your-account-id.r2.cloudflarestorage.com
CLOUDFLARE_R2_ACCESS_KEY=your-r2-access-key
CLOUDFLARE_R2_SECRET_KEY=your-r2-secret-key

# R2 Bucket Names
TEMPO_R2_BUCKET=grafto-tempo-traces
LOKI_R2_BUCKET=grafto-loki-logs

# Grafana Configuration
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=admin
GRAFANA_SECRET_KEY=your-32-char-secret-key-here
GRAFANA_DOMAIN=telemetry.mbvlabs.com

# Prometheus Authentication
PROMETHEUS_ADMIN_USER=prometheus
PROMETHEUS_ADMIN_PASSWORD=your-prometheus-password

# Domain Configuration
DOMAIN=https://telemetry.mbvlabs.com
